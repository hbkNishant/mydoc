INSERT INTO SUBSET_PROPERTY

(SUBSET_PROPERTY_ID, subset_id, name, DESCRIPTION, value, DATA_TYPE)

select SUBSET_PROPERTY_SEQ.nextval, LATEST_REVISION_SUBSET_ID,

       'SoSSource', 'SoS Source', 'CMS', 'string'

from SUBSET_MINOR_VERSION where ACTIVE=1 and SUBSET_IDENTIFIER_ID in

(select SUBSET_IDENTIFIER_ID from CATEGORY_SUBSET_IDENT_ASSOC where CATEGORY_ID in

(select CATEGORY_ID from CATEGORY where

PRODUCT_VERSION_ID =

(select PRODUCT_VERSION_ID FROM PRODUCT_VERSION where PRODUCT_ID =

(select PRODUCT_ID from PRODUCT where PRODUCT_CID = 'MDCR-PROCEDURES') and VERSION_CID = 'RM24')));


INSERT INTO SUBSET_PROPERTY

(SUBSET_PROPERTY_ID, subset_id, name, DESCRIPTION, value, DATA_TYPE)

select SUBSET_PROPERTY_SEQ.nextval, LATEST_REVISION_SUBSET_ID,

       'SoSAge', 'SoS Age', 'ADULT', 'string'

from SUBSET_MINOR_VERSION where ACTIVE=1 and SUBSET_IDENTIFIER_ID in

(select SUBSET_IDENTIFIER_ID from CATEGORY_SUBSET_IDENT_ASSOC where CATEGORY_ID in

(select CATEGORY_ID from CATEGORY where

PRODUCT_VERSION_ID =

(select PRODUCT_VERSION_ID FROM PRODUCT_VERSION where PRODUCT_ID =

(select PRODUCT_ID from PRODUCT where PRODUCT_CID = 'MDCR-PROCEDURES') and VERSION_CID = 'RM24')));

UPDATE SUBSET_PROPERTY set value = 'true' where name = 'SoSLookup' and subset_id in

(select LATEST_REVISION_SUBSET_ID

from SUBSET_MINOR_VERSION where ACTIVE=1 and SUBSET_IDENTIFIER_ID in

(select SUBSET_IDENTIFIER_ID from CATEGORY_SUBSET_IDENT_ASSOC where CATEGORY_ID in

(select CATEGORY_ID from CATEGORY where

PRODUCT_VERSION_ID =

(select PRODUCT_VERSION_ID FROM PRODUCT_VERSION where PRODUCT_ID =

(select PRODUCT_ID from PRODUCT where PRODUCT_CID = 'MDCR-PROCEDURES') and VERSION_CID = 'RM24'))));
